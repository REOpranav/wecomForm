{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://work.weixin.qq.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/Form.html"
    }
  ]
}
